<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
        <Dialog id="updateStoneTime" contentWidth="38%" contentHeight="20%">
            <content>
                <l:VerticalLayout width="100%">
                    <form:Form minWidth="1024" editable="true">
                        <form:layout>
                            <form:ResponsiveGridLayout labelSpanL="3" labelSpanS="3" labelSpanM="3" columnsL="6" columnsM="6" columnsS="6"/>
                        </form:layout>
                        <form:formContainers>
                            <form:FormContainer>
                                <form:FormElement label="Başlangıç Zamanı">
                                    <form:fields>
                                        <DatePicker displayFormat="short" id="idStartDatePicker"/>
                                        <Button  text="Güncelle" type="Accept" press="guncelle"/>
                                    </form:fields>
                                </form:FormElement>
                            </form:FormContainer>
                        </form:formContainers>
                    </form:Form>
                </l:VerticalLayout>
            </content>
            <beginButton/>
            <buttons>
                <Button text="ÇIKIŞ" press="onCancelFrag01"/>
            </buttons>
            <customHeader/>
            <endButton/>
            <subHeader/>
        </Dialog>
    </core:FragmentDefinition>
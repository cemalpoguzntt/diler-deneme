            <Table id="CharacteristicTable4" editable="false" items="{panelsJSON>/3/Y_TLR}" mode="SingleSelectMaster">
                <columns>
                    <Column>
                        <Label text="{i18n>OEE_LABEL_CHARACTERISTIC_DESCRIPTION}" />
                    </Column>
                    <Column visible="false">
                        <Label text="{i18n>OEE_LABEL_CHARACTERISTIC_CODE}" />
                    </Column>
                    <Column>
                        <Label text="{i18n>OEE_LABEL_CHARACTERISTIC_VALUE}" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{panelsJSON>CHARDESC}" />
                            <Text text="{panelsJSON>CHARCODE}" />
                            <Text text="{panelsJSON>CHARVALUE}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </Panel>
        <endButton>
            <Button text="{i18n>OEE_BTN_CLOSE}" press="handleCancel"></Button>
        </endButton>
    </Dialog>
</core:FragmentDefinition>

                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{panelsJSON>CHARDESC}" />
                            <Text text="{panelsJSON>CHARCODE}" />
                            <Text text="{panelsJSON>CHARVALUE}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </Panel>
        <Panel expandable="true" headerText="{i18n>OEE_LABEL_MECHANICAL}">
            <Table id="CharacteristicTable3" editable="false" items="{panelsJSON>/2/Y_MEK}" mode="SingleSelectMaster">
                <columns>
                    <Column>
                        <Label text="{i18n>OEE_LABEL_CHARACTERISTIC_DESCRIPTION}" />
                    </Column>
                    <Column visible="false">
                        <Label text="{i18n>OEE_LABEL_CHARACTERISTIC_CODE}" />
                    </Column>
                    <Column>
                        <Label text="{i18n>OEE_LABEL_CHARACTERISTIC_VALUE}" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>

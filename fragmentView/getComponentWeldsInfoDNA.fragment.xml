<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form">
    <Dialog title="{i18n>OEE_LABEL_SOURCES}" id="getComponentWeldsInfo">
            <Table id="weldTable"  items="{/Row}" width = "300px">
                <columns>
                    <Column hAlign = "Center">
                        <Label text="{i18n>OEE_LABEL_SOURCE_METERAGE}" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{QUANTITY}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        <beginButton>
        </beginButton>
        <endButton>
            <Button text="{i18n>OEE_BTN_CLOSE}" press="onPressCloseComponentsWelds" ></Button>
        </endButton>
    </Dialog>
</core:FragmentDefinition>
